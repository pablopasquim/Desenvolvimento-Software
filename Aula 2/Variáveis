        int i = 10;
        System.out.println(i);

        i += 5;

        System.out.println(i);

        i -= 5;

        System.out.println(i);

        i *= 5;

        System.out.println(i);

        i /= 5;

        System.out.println(i);

        i %= 5;

        System.out.println(i);

        i++;

        System.out.println(i);

        i--;

        System.out.println(i);
    }
    


}
